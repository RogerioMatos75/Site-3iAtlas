{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/hq",
      "destination": "/hq-viewer/hq.html"
    },
    {
      "source": "/hq-viewer/(.*)",
      "destination": "/hq-viewer/$1"
    },
    {
      "source": "/node_modules/(.*)",
      "destination": "/node_modules/$1"
    },
    {
      "source": "/vm-alien/agent-endpoint.js",
      "destination": "/server.js"
    }
  ]
}